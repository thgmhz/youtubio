{
  "manifest_version": 2,
  "name": "Youtubio",
  "description": "",
  "homepage_url": "https://github.com/thiagommedeiros/youtubio",
  "version": "1.0.0",
  "author": "Thiago M. Medeiros",
  "browser_action": {
   "default_icon": "images/icon-mp3.png",
   "default_title": "Convert a video from Youtube to MP3"
  },
  "icons": {
    "16": "images/icon-mp3.png",
    "48": "images/icon-mp3.png",
    "128": "images/icon-mp3.png"
  },
  "background": {
    "scripts": ["scripts/main.js"]
  },
  "permissions": ["tabs","activeTab","http://*/*"]
}
